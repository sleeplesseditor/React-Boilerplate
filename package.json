{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "test": "jest ./src",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleeplesseditor/React-Boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Josh Bryson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sleeplesseditor/React-Boilerplate/issues"
  },
  "homepage": "https://github.com/sleeplesseditor/React-Boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  },
  "dependencies": {
    "jest": "^22.1.4"
  }
}
